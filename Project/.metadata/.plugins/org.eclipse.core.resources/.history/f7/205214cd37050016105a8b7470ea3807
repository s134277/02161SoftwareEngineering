package projectManager;

import java.util.ArrayList;
import java.util.List;

public class System {
	List<User> users = new ArrayList<User>();
	public System(){
		
	}

	public List<User> getUsers() {
		return users;
	}

	public void register(User user) {
		for(User : user; users){
			
		}
		users.add(user);
	}
	
	
}
